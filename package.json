{
  "name": "Tess",
  "version": "0.0.1",
  "description": "Applicant tracking system core and UI",
  "main": "index.js",
  "scripts": {
    "start": "NODE_PATH=./ node -r esm -r dotenv/config index.js",
    "console": "NODE_PATH=./ node -r esm -r dotenv/config",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "npm run knex seed:run",
    "migrate:make": "npm run knex migrate:make",
    "db:migrate": "npm run knex migrate:latest",
    "db:rollback": "npm run knex migrate:rollback",
    "knex": "NODE_PATH=./ NODE_OPTIONS='-r esm' knex --knexfile ./database/config.js",
    "db": "psql tess_development"
  },
  "author": "Damon Aw <daemonsy@gmail.com>",
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "esm": "^3.0.34",
    "express": "^4.16.3",
    "graphiql": "^0.11.11",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.1",
    "knex": "^0.14.6",
    "lodash-es": "^4.17.10",
    "morgan": "^1.9.0",
    "objection": "^1.1.8",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "dotenv": "^5.0.1",
    "faker": "^4.1.0",
    "knex-logger": "^0.1.0"
  }
}
