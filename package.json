{
  "name": "Tess",
  "version": "0.0.1",
  "description": "Applicant tracking system core and UI",
  "main": "index.js",
  "watch": {
    "gql:generate_schema": "graphql",
    "relay": "app"
  },
  "scripts": {
    "start": "NODE_PATH=./ node -r esm -r dotenv/config index.js",
    "start:dev": "NODE_PATH=./ nodemon -r esm -r dotenv/config index.js",
    "start:debug": "NODE_PATH=./ node --inspect -r esm -r dotenv/config index.js",
    "console": "NODE_PATH=./ node -r esm -r dotenv/config",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "npm run knex seed:run",
    "migrate:make": "npm run knex migrate:make",
    "db:migrate": "npm run knex migrate:latest",
    "db:rollback": "npm run knex migrate:rollback",
    "knex": "NODE_PATH=./ NODE_OPTIONS='-r esm' knex --knexfile ./database/config.js",
    "db": "psql tess_development",
    "gql:generate_schema": "NODE_PATH=./ node -r esm -r dotenv/config scripts/generate-schema.js",
    "relay": "relay-compiler --src ./app --schema ./app/schema.json",
    "watch": "npm-watch"
  },
  "author": "Damon Aw <daemonsy@gmail.com>",
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "dataloader": "^1.4.0",
    "esm": "^3.0.34",
    "express": "^4.16.3",
    "graphiql": "^0.11.11",
    "graphql": "0.12.3",
    "graphql-tools": "^3.0.1",
    "knex": "^0.14.6",
    "lodash-es": "^4.17.10",
    "morgan": "^1.9.0",
    "objection": "^1.1.8",
    "pg": "^7.4.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-relay": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-relay": "^1.6.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "faker": "^4.1.0",
    "graphql-relay": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "knex-logger": "^0.1.0",
    "nodemon": "^1.17.4",
    "npm-watch": "^0.3.0",
    "relay-compiler": "^1.6.0",
    "style-loader": "^0.21.0",
    "uuid": "^3.2.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.3"
  }
}
